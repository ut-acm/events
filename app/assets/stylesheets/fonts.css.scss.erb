
@font-face {
    font-family: 'Titillium Web';
    font-weight: normal;
    src: url(<%= asset_path 'titilliumweb-regular-webfont.woff' %>) format('woff'),
    url(/events<%= asset_path 'titilliumweb-regular-webfont.svg' %>) format('svg');
}

@font-face {
    font-family: 'Titillium Web';
    font-weight: bold;
    src: url(<%= asset_path 'titilliumweb-bold-webfont.woff' %>) format('woff'),
    url(/events<%= asset_path 'titilliumweb-bold-webfont.svg' %>) format('svg');
}

@font-face {
    font-family: 'MyriadPro-Regular';
    src: url(<%= asset_path 'myriadproregular.woff' %>) format('woff');
}

@font-face {
    font-family: 'MyriadPro-Light';
    src: url(<%= asset_path 'myriadprolight.woff' %>) format('woff');
}

@font-face {
    font-family: 'Mitra';
    font-style: normal;
    font-weight: 400;
    src: url(<%= asset_url 'BMitra-Regular.woff' %>) format('woff');
}

@font-face {
    font-family: 'Mitra';
    font-style: normal;
    font-weight: 700;
    src: url(<%= asset_url 'BMitra-Bold.woff' %>) format('woff');
}
