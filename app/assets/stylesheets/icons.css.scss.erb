i {
	display: inline-block;
	font-size: 35px;
	height: 32px;
	margin: 5px 0;
	width: 32px;
}

i.audience {
	background: url(<%= asset_data_uri 'i-audience.png' %>) no-repeat;
	background-size: 32px 32px;
}

i.check {
	background: url(<%= asset_data_uri 'i-check.png' %>) no-repeat;
	background-size: 32px 32px;
}

i.link {
	background: url(<%= asset_data_uri 'i-link.png' %>) no-repeat;
	background-size: 32px 32px;
}
