

<table>
  <thead>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>email</th>
    <th>credit</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.id%></td>
        <td><%= link_to profile.credentials, profile %> </td>
        <td><%= profile.user.email %> </td>
        <td><%= profile.credit %> </td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>


