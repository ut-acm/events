<h1>Listing models</h1>

<table>
  <thead>
    <tr>
      <th>Payit</th>
      <th>Name</th>
      <th>Surname</th>
      <th>Mobile</th>
      <th>Email</th>
      <th>Grades image</th>
      <th>Region type</th>
      <th>Exam regional rank</th>
      <th>Exam overall rank</th>
      <th>City</th>
      <th>School</th>
      <th>Payment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @models.each do |model| %>
      <tr>
        <td><%= model.PayIt %></td>
        <td><%= model.name %></td>
        <td><%= model.surname %></td>
        <td><%= model.mobile %></td>
        <td><%= model.email %></td>
        <td><%= model.grades_image %></td>
        <td><%= model.region_type %></td>
        <td><%= model.exam_regional_rank %></td>
        <td><%= model.exam_overall_rank %></td>
        <td><%= model.city %></td>
        <td><%= model.school %></td>
        <td><%= model.payment_id %></td>
        <td><%= link_to 'Show', model %></td>
        <td><%= link_to 'Edit', edit_model_path(model) %></td>
        <td><%= link_to 'Destroy', model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Model', new_model_path %>
