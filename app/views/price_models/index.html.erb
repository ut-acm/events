<h1>Listing price_models</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Name</th>
      <th>Event</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @price_models.each do |price_model| %>
      <tr>
        <td><%= price_model.price %></td>
        <td><%= price_model.name %></td>
        <td><%= price_model.event_id %></td>
        <td><%= link_to 'Show', price_model %></td>
        <td><%= link_to 'Edit', edit_price_model_path(price_model) %></td>
        <td><%= link_to 'Destroy', price_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Price model', new_price_model_path %>
